@model TeenCodeWebsite.Models.SMLink

@using (Html.BeginForm("AddLink", "Social"))
{
    <div class="form-group">
        @Html.LabelFor(m => m.Name)
        @Html.TextBoxFor(m => m.Name, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.PageUrl)
        @Html.TextBoxFor(m => m.PageUrl, new { @class = "form-control" })
    </div>

    <div class="form-group">
        @Html.LabelFor(m => m.ImageUrl)
        @Html.TextBoxFor(m => m.ImageUrl, new { @class = "form-control" })
    </div>

    <button type="submit">Submit</button>
}

